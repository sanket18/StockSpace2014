<form:form class="form-horizontal" role="form" method="POST" action="../update" modelAttribute="product">

					<div class="form-group">
						<label for="inputName" class="col-sm-2 control-label">Product ID:</label>
						<div class="col-sm-10">
							<form:input class="form-control" path="productId" id="productId" placeholder="Enter productId" required="required"></form:input>
						</div>
					</div> 
					 
